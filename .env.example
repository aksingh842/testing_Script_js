ACCESS_TOKEN=your_access_token_here
REFRESH_TOKEN=your_refresh_token_here
COMPANY_ID=your_company_id_here
WEBHOOK_URL=your_webhook_url_here

# Model Monitoring Configuration
# Set to "onprem" to enable CPU/Memory/iGPU monitoring, or "api" to skip monitoring
MODEL_TYPE=api
# Name or PID of the model process to monitor (e.g., "python", "uvicorn", "main.py") - Linux only
MODEL_PROCESS_NAME=python

# HWiNFO Log File Path (Windows only - requires HWiNFO with Shared Memory Support enabled)
HWINFO_LOG_FILE=c:\path\to\hwinfo_log.csv
